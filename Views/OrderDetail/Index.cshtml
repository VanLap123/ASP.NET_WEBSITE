@model IEnumerable<WEBGROUP_GCC0903.Models.OrderDetail>

@{
    ViewData["Title"] = "Index";
}

<h1>Order Detail</h1>

<p>
    <a class="btn btn-dark" asp-action="Create">Add</a>
</p>
<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">
                Quantity
            </th>
            <th scope="col">
                Order ID
            </th>
            <th scope="col">
                Product ID
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.quantity
            </td>
            <td>
                @item.Order.order_id
            </td>
            <td>
                @item.Product.pro_id
            </td>
        </tr>
}
    </tbody>
</table>
