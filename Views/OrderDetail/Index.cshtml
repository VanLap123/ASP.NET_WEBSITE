@model IEnumerable<WEBGROUP_GCC0903.Models.OrderDetail>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Quantity
            </th>
            <th>
                Order ID
            </th>
            <th>
                Product ID
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.quantity
            </td>
            <td>
                @item.Order.order_id
            </td>
            <td>
                @item.Product.pro_id
            </td>
        </tr>
}
    </tbody>
</table>
